{
  "extends": ["airbnb-typescript/base"],
  "plugins": ["jest", "eslint-plugin-import"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "import/prefer-default-export": 0,
    "max-len": "off"
  }
}